class Solution:
    def func(self,x):
        if x % 4 == 0:
            return x
        elif x % 4 == 1:
            return 1
        elif x % 4 == 2:
            return x + 1
        elif x % 4 == 3:
            return 0

    def findXOR(self,l, r):
        return self.func(l - 1) ^ self.func(r)
